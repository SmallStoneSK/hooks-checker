{
  "name": "hooks-checker",
  "version": "0.0.1",
  "description": "check whether react hooks such as useEffect missing second argument",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc --project tsconfig.json",
    "test": "ava --tap | tap-nyan",
    "test-notap": "ava",
    "prepublishOnly": "npm run build"
  },
  "author": "SmallStoneSK",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "ava": "^4.0.1",
    "rimraf": "^3.0.2",
    "tap-nyan": "^1.1.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@babel/parser": "^7.17.3",
    "@babel/traverse": "^7.17.3",
    "@babel/types": "^7.17.0",
    "fs-extra": "^10.0.0"
  },
  "ava": {
    "files": [
      "tests/**/*.spec.*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
